<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>Semi_ATG</display-name>
  <welcome-file-list>
    <welcome-file>main.jsp</welcome-file>
    
  </welcome-file-list>
  
  <servlet>
	<servlet-name>Notice</servlet-name>
	<servlet-class>com.atg.controller.NoticeController</servlet-class>
  </servlet>  
  <servlet-mapping>
	<servlet-name>Notice</servlet-name>
	<url-pattern>/NoticeController.do</url-pattern>
  </servlet-mapping>
  
  
   <servlet>
	<servlet-name>Member</servlet-name>
	<servlet-class>com.atg.controller.MemberController</servlet-class>
  </servlet>  
  <servlet-mapping>
	<servlet-name>Member</servlet-name>
	<url-pattern>/MemberController.do</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
	<servlet-name>Cal</servlet-name>
	<servlet-class>com.atg.controller.CalController</servlet-class>
  </servlet>  
  <servlet-mapping>
	<servlet-name>Cal</servlet-name>
	<url-pattern>/CalController.do</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>Weather</servlet-name>
    <servlet-class>com.atg.controller.WeatherController</servlet-class>
  </servlet>  
  <servlet-mapping>
    <servlet-name>Weather</servlet-name>
    <url-pattern>/WeatherController.do</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>register</servlet-name>
    <servlet-class>com.atg.controller.RegisterController</servlet-class>
  </servlet> 
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/RegisterController.do</url-pattern>
  </servlet-mapping>
  
</web-app>